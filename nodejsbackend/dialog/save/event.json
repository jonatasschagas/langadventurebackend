          "$$hashKey": "object:54",
          "translation": "",
          "next": [
            {
              "id": "e8338211-b794-cbc5-fbf0-dec68d919d2e",
              "parent": "d413f9f2-a386-51ee-0c73-e1f4acae101b",
              "text": "Alright! So, who's your friend?",
              "translation": "",
              "type": "question",
              "answers": [
                {
                  "id": "2450bf7c-9077-f1d0-b4b5-abe356735de6",
